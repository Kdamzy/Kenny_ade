#include<stdio.h>

int main()
{
    //Declare variables to store the input numbers and the results
    int num1, num2, difference;

    //Ask user to input the first number
    printf("Enter the first number: ");
    scanf("%d", &num1);
    //Ask user to input the second number
    printf("Enter the second number: ");
    scanf ("%d", &num2);

    //Find the difference of two numbers
    difference = num1 - num2;

    //Print the result
    printf("The difference of your inputs is:%d", difference);

    return 0;
}
